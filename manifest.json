{
    "name": "WhatsApp Web Dark",
    "version": "1.0",
    "description": "A chrome extension to enable dark mode in WhatsApp Web.",
    "permissions": [
        "activeTab",
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "src/background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "src/popup.html"
    },
    "manifest_version": 2
}